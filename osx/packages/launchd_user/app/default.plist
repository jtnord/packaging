<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
  <dict>
    <key>StandardOutPath</key>
    <string>/var/log/@@ARTIFACTNAME@@/@@ARTIFACTNAME@@.log</string>
    <key>StandardErrorPath</key>
    <string>/var/log/@@ARTIFACTNAME@@/@@ARTIFACTNAME@@.log</string>
    <key>EnvironmentVariables</key>
    <dict>
      <key>JENKINS_HOME</key>
      <string>/Users/Shared/@@CAMELARTIFACTNAME@@/Home</string>
    </dict>
    <key>GroupName</key>
    <string>daemon</string>
    <key>KeepAlive</key>
    <true/>
    <key>Label</key>
    <string>@@OSX_IDPREFIX@@</string>
    <key>ProgramArguments</key>
    <array>
      <string>/bin/bash</string>
      <string>/Library/Application Support/@@CAMELARTIFACTNAME@@/jenkins-runner.sh</string>
    </array>
    <key>RunAtLoad</key>
    <true/>
    <key>UserName</key>
    <string>@@ARTIFACTNAME@@</string>
    <key>SessionCreate</key>
    <true/>
  </dict>
</plist>
